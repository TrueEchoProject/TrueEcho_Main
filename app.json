{
  "expo": {
    "name": "TrueEcho",
    "slug": "trueecho",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "trueecho",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanseo.TrueEcho",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleMapsApiKey": "AIzaSyCRk_PvJvc1YDjdUffsVF-gYDtmmOjw4cE"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby places",
        "NSLocationAlwaysUsageDescription": "We need your location to show nearby places",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to show nearby places"
      }
    },
    "android": {
      "package": "com.hanseo.TrueEcho",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCRk_PvJvc1YDjdUffsVF-gYDtmmOjw4cE"
        }
      },
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "trueecho",
    "extra": {
      "eas": {
        "projectId": "71a30e64-a2a8-4e53-888b-f53592590986"
      }
    }
  }
}
